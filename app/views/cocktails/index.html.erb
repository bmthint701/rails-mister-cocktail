<div class= "banner"  style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)),
   url('<%= cl_image_path("blue_bar.0.jpg")%>')">
  <h1>Cocktails!</h1>
</div>
<%= link_to "Add Cocktail", new_cocktail_path %>
<ul>
  <% @cocktails.each do |cocktail| %>
